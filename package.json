{
  "name": "floating-vue-monorepo",
  "packageManager": "pnpm@8.2.0",
  "version": "2.0.0-beta.20",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter=\"./packages/*\" run build",
    "lint": "pnpm -r run lint",
    "release": "pnpm run lint && pnpm run build && sheep release -b main"
  },
  "devDependencies": {
    "@akryum/sheep": "^0.5.0"
  }
}
